def powerval(x):
    c=x*x
    return c
    
    

y=6
y=powerval(y)
print(y)



def addnumbber(a, *b):
    
    c=a
    for i in b:
        c=c+i
        
    print(c)
    
    
addnumbber(3,5,6,7,8,9,8,6,7,8,8,8,8,8,8,8,8,8,8,8,7,6,5,5,45,4,34,3,3,3,4,54,5,6,7,6,5,5,6,7,5,4,3,23,3,4,65,7,7)

def person(name, *dat):
    print(dat)
    
person('deshan', 23,'sri lanka', 234)

